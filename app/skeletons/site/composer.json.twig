{
    "name": "vendor-name/package-name",
    "description": "",
    "keywords": ["spress", "theme"],
    "type": "spress-theme",
    {% if requires|length > 0 %}
    "require": {
        {% for name, version in requires %}
        "{{ name }}": "{{ version|raw }}"
        {% endfor %}
    },
    {% endif %}
    "minimum-stability": "dev"
}
